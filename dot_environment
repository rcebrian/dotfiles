CLI_EDITOR='vim'
GIT_EDITOR=$CLI_EDITOR

# user env

# bin
LOCAL_BIN=$HOME/.local/bin
export PATH=$PATH:$LOCAL_BIN

# golang
GOROOT=/usr/local/go
GOPATH=$HOME/go
export PATH=$PATH:$GOROOT/bin:$GOPATH/bin
